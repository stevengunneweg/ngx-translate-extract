{
  "name": "@gunnaway/ngx-translate-extract",
  "version": "8.0.4",
  "description": "Extract strings from projects using ngx-translate",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "bin/",
    "dist/"
  ],
  "type": "module",
  "bin": {
    "ngx-translate-extract": "bin/cli.js"
  },
  "scripts": {
    "publish:next": "npm run build && npm publish --tag next --access public",
    "build": "npm run clean && tsc",
    "watch": "npm run clean && tsc --watch",
    "clean": "rimraf ./dist",
    "lint": "tslint --force './src/**/*.ts'",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.spec.json\" mocha -r ts-node/register tests/**/*.spec.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "prettier": {
    "trailingComma": "none",
    "printWidth": 145,
    "useTabs": true,
    "singleQuote": true
  },
  "lint-staged": {
    "{src,tests}/**/*.{ts}": [
      "tslint --project tsconfig.json -c tslint.commit.json --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevengunneweg/ngx-translate-extract.git"
  },
  "keywords": [
    "angular",
    "ionic",
    "ngx-translate",
    "extract",
    "extractor",
    "translate",
    "translation",
    "i18n",
    "gettext"
  ],
  "author": "Kim Biesbjerg <kim@biesbjerg.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevengunneweg/ngx-translate-extract/issues"
  },
  "homepage": "https://github.com/stevengunneweg/ngx-translate-extract",
  "engines": {
    "node": ">=12"
  },
  "config": {},
  "devDependencies": {
    "@angular/compiler": "14",
    "@types/braces": "3.0.1",
    "@types/chai": "4.3.3",
    "@types/flat": "5.0.2",
    "@types/gettext-parser": "4.0.1",
    "@types/glob": "8.0.0",
    "@types/mocha": "9.1.1",
    "@types/node": "18.7.18",
    "braces": "3.0.2",
    "chai": "4.3.6",
    "cross-env": "7.0.3",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "mocha": "10.0.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-etc": "1.13.10",
    "typescript": "4.8.3"
  },
  "peerDependencies": {
    "@angular/compiler": ">=13.0.0",
    "typescript": ">=4.4.0"
  },
  "dependencies": {
    "@angular/compiler": "13.1.1",
    "@phenomnomnominal/tsquery": "5.0.0",
    "boxen": "7.0.0",
    "colorette": "2.0.19",
    "flat": "5.0.2",
    "gettext-parser": "6.0.0",
    "glob": "8.0.3",
    "path": "0.12.7",
    "terminal-link": "3.0.0",
    "yargs": "17.5.1"
  }
}